.rightcontent.space-20px
    .inner-content
        .col-1of1
            .col-content
                .breadcrumb
                    a.onecrumb(href="index-logined.html")
                        i.fa.fa-home
                        |&nbsp;门户首页
                    span
                        i.fa.fa-angle-right
                    .onecrumb 信息查询
                    span
                        i.fa.fa-angle-right
                    .onecrumb 获证信息
    form.form-control.box-form.has-title
        .box-form-title 过滤查询
        .inner-content
            .col-1of3
                .col-content
                    .box-form-block
                        .box-form-label.control-label 证件类型：
                        select.input-select
                            option(value="0") 全部
                            option(value="1") 类型1
                            option(value="2") 类型2
                            option(value="3") 类型3
                            option(value="4") 类型4
            .col-2of3
                .col-content
                    .box-form-block
                        .box-form-label.control-label 获得企业：
                        input(type="text" placeholder="企业名称").input-text
            .col-8of9
                .col-content
                    .box-form-block
                        .box-form-label.control-label 获得起止时间：
                        .col-6of13
                            input(type="text" placeholder="开始时间").input-text
                        .col-1of13
                            .control-label.text-center.no-left.font-gray-99
                                i.fa.fa-arrows-h
                        .col-6of13
                            input(type="text" placeholder="结束时间").input-text
            .col-1of9
                .col-content
                    .btn.btn-circle2px.bg-blue-ocean.bg-hover-darken.btn-block
                        i.fa.fa-search
                        |&nbsp;查询
    .box-title
        i.fa.fa-certificate
        |&nbsp;获证信息
        .pull-right.mr4
            a.btn.btn-circle2px.bg-blue-ocean.bg-hover-darken(href="#")
                i.fa.fa-plus
                |&nbsp;添加
            .btn.btn-circle2px.bg-blue-ocean.bg-hover-darken
                i.fa.fa-remove
                |&nbsp;批量删除
    include ../table/table-notice
    .box-content
        include ../pagination/index
