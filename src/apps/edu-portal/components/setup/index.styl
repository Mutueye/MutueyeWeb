.setup-content
    width SETUP_CONT_WIDTH
    height SETUP_CONT_HEIGHT
    position absolute
    bottom 0
    left 50%
    margin-left -0.5*SETUP_CONT_WIDTH
    display none
    //background-color GRAY_F5
    //padding SETUP_SPACE 0.5*SETUP_SPACE 0 0.5*SETUP_SPACE
    &.layout-content
        font-size inherit
        //background-color alpha(BLACK,0.5)
        //border-radius COMMON_BORDER_RADIUS
.theme-box
    margin 0.5*SETUP_SPACE
    display block
    float left
    width THEMEBOX_WIDTH
    height THEMEBOX_HEIGHT
    border-radius COMMON_BORDER_RADIUS + 2px
    position relative
    -webkit-appearance: none;
    border none
    outline none
    cursor pointer
    box-shadow 0px 5px 30px 0px alpha(BLACK,0.5)
    z-index 1
    border 2px solid THEMEBOX_BORDER_COLOR
    box-sizing border-box
    img
        width 100%
        height 100%
        float left
        border-radius COMMON_BORDER_RADIUS
    .theme-title
        line-height 0.25*THEMEBOX_HEIGHT
        width 100%
        position absolute
        text-align center
        font-size 0.1*THEMEBOX_HEIGHT
        background-color alpha(BLACK,0.3)
        bottom 0
        color FONT_COLOR_COMMON
        border-bottom-left-radius COMMON_BORDER_RADIUS
        border-bottom-right-radius COMMON_BORDER_RADIUS
    &:hover
        z-index 2

.setup_toggle
    width SETUP_TOGGLE_WIDTH
    height 100px
    position absolute
    left -0.5*SETUP_TOGGLE_WIDTH
    top SETUP_TOGGLE_TOP
    display block
.setup_toggle_btn
    cursor pointer
    width 100%
    height SETUP_TOGGLE_BTN_HEIGHT
    line-height SETUP_TOGGLE_BTN_HEIGHT
    font-size 0.35*SETUP_TOGGLE_BTN_HEIGHT
    text-align center
    color FONT_COLOR_COMMON
    margin-bottom SETUP_SPACE
    display block
    position relative
    text-shadow 0 0 2px alpha(BLACK,0.2)
    .deco-line
        width 100%
        height 1px
        position absolute
        bottom 0
        background-image url(IMG_DIR + 'toggle-deco.png')
        background-position center center
        background-size 100% 1px
        z-index 1
    .deco-sel
        display none
        width 100%
        height 100%
        background-image url(IMG_DIR + 'toggle-deco-sel.png')
        position absolute
        background-size 100% 100%
        background-position center center
        bottom -40%
    &.sel
        color FONT_COLOR_COMMON_ACTIVE
        .deco-sel
            display block

.layout-box
    display block
    width LAYOUTBOX_WIDTH
    height LAYOUTBOX_HEIGHT
    margin 0.5*SETUP_SPACE
    float left
    position relative
    cursor pointer
    &.btn-add
        border-radius COMMON_BORDER_RADIUS
        background-color alpha(WHITE,0.2)
        line-height LAYOUTBOX_HEIGHT
        text-align center
        font-size 0.4*LAYOUTBOX_HEIGHT
        color FONT_COLOR_COMMON
    .bn-block
        box-shadow 0 0 0.1rem 0 alpha(BLACK,0.5)
    for i in (1..BN_COL_NUM)
        for j in (1..BN_ROW_NUM)
            .bn-size-{i}x{j}
                width i * (LAYOUT_BLOCK_WIDTH + LAYOUT_BLOCK_SPACE) - LAYOUT_BLOCK_SPACE
                height j * (LAYOUT_BLOCK_HEIGHT + LAYOUT_BLOCK_SPACE) - LAYOUT_BLOCK_SPACE
            .bn-pos-col-{i}
                left (i - 1)*(LAYOUT_BLOCK_WIDTH + LAYOUT_BLOCK_SPACE)
            .bn-pos-row-{j}
                top (j - 1)*(LAYOUT_BLOCK_HEIGHT + LAYOUT_BLOCK_SPACE)
            .bn-pos-col{i}-row{j}
                left (i - 1)*(LAYOUT_BLOCK_WIDTH + LAYOUT_BLOCK_SPACE)
                top (j - 1)*(LAYOUT_BLOCK_HEIGHT + LAYOUT_BLOCK_SPACE)

.editlayout-container
    display block
    width ELB_CONTAINER_WIDTH
    height ELB_CONTAINER_HEIGHT
    margin-left -0.5*ELB_CONTAINER_WIDTH
    margin-top -0.5*ELB_CONTAINER_HEIGHT + ELB_TOP_OFFSET
    left 50%
    top 50%
    position absolute
    //cursor pointer
    &.btn-add
        border-radius COMMON_BORDER_RADIUS
        background-color alpha(WHITE,0.2)
        line-height LAYOUTBOX_HEIGHT
        text-align center
        font-size 0.4*LAYOUTBOX_HEIGHT
        color FONT_COLOR_COMMON
    .bn-block
        box-shadow 0 0 0.1rem 0 alpha(BLACK,0.5)
        &.bn-color-empty
            background-color alpha(WHITE,0.2)
    for i in (1..BN_COL_NUM)
        for j in (1..BN_ROW_NUM)
            .bn-size-{i}x{j}
                ELB_SIZE_W = i*(ELB_WIDTH + ELB_SPACE) - ELB_SPACE
                ELB_SIZE_H = j*(ELB_HEIGHT + ELB_SPACE) - ELB_SPACE
                width ELB_SIZE_W
                height ELB_SIZE_H
                &.bn-layout-empty
                &.bn-block-ghost
                    .bn-icon
                        width 100%
                        line-height ELB_SIZE_H
                        font-size 0.3*ELB_SIZE_H
                        color alpha(WHITE,0.4)
                &.bn-layout-1
                &.bn-layout-2
                &.bn-layout-3
                &.bn-layout-4
                    .bn-icon
                        width 100%
                        left 0
                        top 50%
                        if i >= j
                            font-size 0.45*ELB_SIZE_H
                            line-height 0.45*ELB_SIZE_H
                            margin-top -0.5*0.45*ELB_SIZE_H - 0.1*ELB_HEIGHT
                        else
                            font-size 0.45*ELB_SIZE_W
                            line-height 0.45*ELB_SIZE_W
                            margin-top -0.5*0.45*ELB_SIZE_W - 0.1*ELB_HEIGHT
                    .bn-title
                        width 100%
                        text-align center
                        left 0
                        line-height 0.25*ELB_HEIGHT
                        font-size 0.15*ELB_HEIGHT
                    .bn-text
                        display none
                &.bn-layout-1
                    .bn-title
                        bottom 0.1*ELB_HEIGHT
                &.bn-layout-2
                    .bn-title
                        background-color alpha(BLACK,15%)
                        bottom 0
                &.bn-layout-3
                    .bn-title
                        text-align left
                        bottom 0.1*ELB_HEIGHT
                        padding-left 0.18*ELB_HEIGHT
                &.bn-layout-4
                    .bn-title
                        text-align right
                        bottom 0.1*ELB_HEIGHT
                        padding-right 0.18*ELB_HEIGHT

                &.bn-layout-5
                    .bn-icon
                        width 100%
                        left 0
                        top 0
                        margin-top 0.15*ELB_SIZE_H
                        if j <= 1
                            font-size 0.3*ELB_SIZE_H
                            line-height 0.3*ELB_SIZE_H
                        else
                            font-size 0.45*ELB_WIDTH
                            line-height 0.45*ELB_WIDTH
                    .bn-title
                        width 100%
                        text-align center
                        left 0
                        line-height 0.25*ELB_HEIGHT
                        font-size 0.15*ELB_HEIGHT
                        if j <= 1
                            top 0.45*ELB_SIZE_H
                        else
                            top  0.48*ELB_WIDTH + 0.15*ELB_SIZE_H
                    .bn-text
                        width 100%
                        padding 0 0.1*ELB_SIZE_W
                        box-sizing border-box
                        left 0
                        text-align center
                        line-height 0.15*ELB_HEIGHT
                        font-size 0.1*ELB_HEIGHT
                        overflow hidden
                        if j <= 1
                            height 0.15*ELB_HEIGHT
                            top 0.45*ELB_SIZE_H + 0.25*ELB_HEIGHT
                        else
                            height 0.45*ELB_HEIGHT
                            top  0.48*ELB_WIDTH + 0.15*ELB_SIZE_H + 0.25*ELB_HEIGHT
                &.bn-layout-6
                &.bn-layout-7
                    .bn-icon
                        height ELB_SIZE_H
                        line-height ELB_SIZE_H
                        left 0.1*ELB_SIZE_W
                        text-align center
                        if i > j
                            width 0.4*ELB_SIZE_H
                            font-size 0.4*ELB_SIZE_H
                        else
                            width 0.22*ELB_SIZE_W
                            font-size 0.22*ELB_SIZE_W
                    .bn-title
                        font-size 0.15*ELB_HEIGHT
                        text-align left
                        if i > j
                            width 0.75*ELB_SIZE_W - 0.5*ELB_SIZE_H
                            left 0.15*ELB_SIZE_W + 0.5*ELB_SIZE_H
                        else
                            width 0.45*ELB_SIZE_W
                            left 0.45*ELB_SIZE_W

                &.bn-layout-6
                    .bn-title
                        line-height ELB_SIZE_H
                        height ELB_SIZE_H
                    .bn-text
                        display none
                &.bn-layout-7
                    .bn-title
                        line-height 0.2*ELB_HEIGHT
                        height 0.2*ELB_HEIGHT
                        if j < 2
                            top 0.15*ELB_SIZE_H
                        else
                            top 0.3*ELB_SIZE_H
                    .bn-text
                        line-height 0.15*ELB_HEIGHT
                        font-size 0.1*ELB_HEIGHT
                        text-align justify
                        overflow hidden
                        if j < 2
                            height 0.45*ELB_HEIGHT
                            top 0.15*ELB_SIZE_H + 0.22*ELB_HEIGHT
                        else
                            height 0.6*ELB_HEIGHT
                            top 0.3*ELB_SIZE_H + 0.22*ELB_HEIGHT
                        if i > j
                            width 0.75*ELB_SIZE_W - 0.5*ELB_SIZE_H
                            left 0.15*ELB_SIZE_W + 0.5*ELB_SIZE_H
                        else
                            width 0.45*ELB_SIZE_W
                            left 0.45*ELB_SIZE_W

            .bn-pos-col-{i}
                left (i - 1)*(ELB_WIDTH + ELB_SPACE)
            .bn-pos-row-{j}
                top (j - 1)*(ELB_HEIGHT + ELB_SPACE)
            .bn-pos-col{i}-row{j}
                left (i - 1)*(ELB_WIDTH + ELB_SPACE)
                top (j - 1)*(ELB_HEIGHT + ELB_SPACE)

