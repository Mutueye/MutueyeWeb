            cursor default
            .sel-box
                display none !important
        .editlayout-size-item-block
            border-radius COMMON_BORDER_RADIUS
            display block
            position absolute
            background-color alpha(WHITE,0.2)
            &.sel
                background-color alpha(WHITE,0.8)
            &.elsib-1x1
                width ELB_SIZE_BLOCK_W
                height ELB_SIZE_BLOCK_H
            &.elsib-1x2
                width ELB_SIZE_BLOCK_W
                height 2*ELB_SIZE_BLOCK_H + ELB_SIZE_SPACE
            &.elsib-2x1
                width 2*ELB_SIZE_BLOCK_W + ELB_SIZE_SPACE
                height ELB_SIZE_BLOCK_H
            &.elsib-2x2
                width 2*ELB_SIZE_BLOCK_W + ELB_SIZE_SPACE
                height 2*ELB_SIZE_BLOCK_H + ELB_SIZE_SPACE
            &.elsib-col1-row1
                left 0
                top 0
            &.elsib-col1-row2
                left 0
                top ELB_SIZE_BLOCK_H + ELB_SIZE_SPACE
            &.elsib-col2-row1
                left ELB_SIZE_BLOCK_W + ELB_SIZE_SPACE
                top 0
            &.elsib-col2-row2
                top ELB_SIZE_BLOCK_H + ELB_SIZE_SPACE
                left ELB_SIZE_BLOCK_W + ELB_SIZE_SPACE

