.editlayout-popbox
    display none
    position absolute
    width EDITLAYOUT_POP_WIDTH
    height EDITLAYOUT_POP_HEIGHT
    top EDITLAYOUT_POP_TOP
    left 50%
    margin-left -0.5*EDITLAYOUT_POP_WIDTH
    border-radius COMMON_BORDER_RADIUS
    font-size EDITLAYOUT_POP_FONT
    line-height EDITLAYOUT_POP_HEIGHT
    color EDITLAYOUT_POP_FONT_COLOR
    text-align center
    background-color EDITLAYOUT_BG_COLOR
    box-shadow 0 EDITLAYOUT_POP_SHADOW_OFFSET EDITLAYOUT_POP_SHADOW_SIZE 0 alpha(BLACK,0.5)
    z-index 100
    cursor default
    transition all COMMON_ANIM_TIME ease-in-out
    .editlayout_popbtn
        width auto
        height EDITLAYOUT_POP_BTN_HEIGHT
        line-height EDITLAYOUT_POP_BTN_HEIGHT
        box-sizing border-box
        border 1px solid GRAY_E3
        color GRAY_66
        text-align center
        float left
        cursor pointer
        margin EDITLAYOUT_PADDING
        border-radius 0.8*COMMON_BORDER_RADIUS
        &.editlayout_popbtn_1
            width EDITLAYOUT_POPBTN_WIDTH_1
        &.editlayout_popbtn_2
            width EDITLAYOUT_POPBTN_WIDTH_2
            margin-left 0
        &:hover
            color GRAY_33
            background-color GRAY_F0
    .editlayout_poparrow
        position absolute
        border-arrow(EDITLAYOUT_BG_COLOR,EDITLAYOUT_POP_ARROW_SIZE,down)
        left 50%
        margin-left -1*EDITLAYOUT_POP_ARROW_SIZE
        bottom auto
        top 100%
        transition all COMMON_ANIM_TIME ease-in-out
    &:hover
        background-color EDITLAYOUT_BG_COLOR_ACTIVE
        .editlayout_poparrow
            border-arrow(EDITLAYOUT_BG_COLOR_ACTIVE,EDITLAYOUT_POP_ARROW_SIZE,down)
.bn-block:hover
    .editlayout-popbox
        display block
        transform-origin 50% 100%
        animation show_edit_pop COMMON_ANIM_TIME ease-out forwards


@keyframes show_edit_pop
    0%
        transform scale(0)
    100%
        transform scale(1)
