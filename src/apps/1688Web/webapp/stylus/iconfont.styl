
@font-face
  font-family "iconfont"
  src url( FONT_DIR + 'iconfont.eot')
  src url( FONT_DIR + 'iconfont.eot') format('embedded-opentype'), url( FONT_DIR + 'iconfont.woff') format('woff'), url( FONT_DIR + 'iconfont.ttf') format('truetype'), url( FONT_DIR + 'iconfont.svg') format('svg')

.iconfont
  font-family "iconfont" !important
  font-size inherit
  font-style normal
  -webkit-font-smoothing  antialiased
  -webkit-text-stroke-width  0.2px
  -moz-osx-font-smoothing  grayscale

.icon-return-arrow:before { content: "\e61d"; }

.icon-return:before { content: "\e654"; }

.icon-arrow-left:before { content: "\e656"; }

.icon-more:before { content: "\e65c"; }

.icon-arrow-right:before { content: "\e65e"; }

.icon-news:before { content: "\e667"; }

.icon-book:before { content: "\e66c"; }

.icon-building:before { content: "\e6b2"; }
