extends layout

block css
    link(rel='stylesheet', href= CSS_DIR + 'font-awesome.min.css')
    link(rel='stylesheet', href= CSS_DIR + 'index-m.css')
        

block content
    .main-content
        include ../components/topbar/index-m
        .main-body
            .topbar-space
            include ../components/banner/banner-chuangye-train
            .container.tp-container
                .tp-content
                    .deco-box
                    .tp-content-title
                        |创业培训详情
                        .tp-content-title-deco
                            |中艺1688
                            br
                            |SHANDONG ARTEX
                    .tp-content-inner
                        .row.row-no-bottom
                            .col-xs-12
                                .orgintro-container.has-bottom
                                    .orgintro-logo
                                        img(src=IMG_DIR + 'rcs-item-04.jpg')
                                    .orgintro-title 创业培训名称
                                    .orgintro-content 创业培训简介...是以智慧教育生态运营为主的高新技术企业，采用“产品研发+投资承建+系统集成”为一体的现代化综合信息服务体系，为用户提供最佳信息化应用服务体验。2016年1月公司正式在全国中小企业股份转让系统（新三板）挂牌上市，股票代码为835715。
                                    .orgintro-pills
                                        a.btn.orgintro-pill.orgintro-btn(href="m-chuangye-train-submit.html") 我要报名
                            .col-xs-12.mt-space
                                .section-title
                                    .section-title-icon
                                    .section-title-text.bg-white 创业培训详情
                                .article-content
                                    p 创业培训文章内容
        include ../components/m-footnav/index

block js
    script(src=JS_DIR + 'jquery.min.js')
    script(src=JS_DIR + 'bootstrap.min.js')
    script(src=JS_DIR + 'bootstrapvalidator/bootstrapValidator.min.js')
    script(src=JS_DIR + 'bootstrapvalidator/zh_CN.js')
    script(src=JS_DIR + 'toastr.min.js')
    script(src=JS_DIR + 'BSModal.js')
    script(src=JS_DIR + 'common.js')
    script(src=JS_DIR + 'app-m.js')
